{
    "headers": [
        {
            "source": "/",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600, s-maxage=3600"
                }
            ]
        },
        {
            "source": "/:path([^/]+/?)?",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600, s-maxage=3600"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/:path(craft|feats|journey|contact)?",
            "destination": "/:path",
            "statusCode": 200
        },
        {
            "source": "/:part/:path(craft|feats|journey|contact)?",
            "destination": "/:path",
            "statusCode": 307
        },
        {
            "source": "/:path*",
            "destination": "/",
            "statusCode": 404
        }
    ]
}